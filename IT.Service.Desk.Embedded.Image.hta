							   'Single or multiple users effected: ' + '\n\n' +
							   'Particular site or department effected: ' + '\n\n' +
							   '--------------------Sending Workstation Information--------------------' + '\n' +
							   'User Name: ' + uname +'\n' + 
					           'Computer Name: ' + cname +'\n'+ 'IP address: ' + ip + '\n' + 
							   'MAC address: ' + mac + '\n' + 'PC Serial#: ' + serial + '\n' + 'RoutingRef=EUS' + '\n\n' ;							 
							   location.href = 'mailto:' + email
						+ '?subject=' + encodeURIComponent(subject)
						+ '&body=' + encodeURIComponent(body);
				}
	</script>

